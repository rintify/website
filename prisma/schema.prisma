datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            Int @id @default(autoincrement())
  name          String @unique
  nickName      String @default("")
  passwordHash  String @default("")
  comment       String @default("")
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
